<Alertas_Mensaje@ModalView>:
    titulo: 'Sin Titulo'
    contenido: 'Sin Contenido'
    bg: (1,0,0,1)
    size_hint: (0.9,0.7)
    canvas.before:
        Color:
            rgba: 0,0,0,1
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [dp(10)]
        Color:
            rgba: root.bg
        RoundedRectangle:
            size: self.size[0]-2, self.size[1]-2
            pos: self.x+1,self.y+1
            radius: [dp(10)]
    BoxLayout:
        orientation: 'vertical'
        Label:
            text: root.titulo
            size_hint_y: None
            height: dp(30)
            font_size: self.height*0.7
            color: 1,1,1,1
            canvas.before:
                Color:
                    rgba: 0,0,0,1
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [dp(10),0,dp(10),0]

        Label:
            text: root.contenido
            font_size: dp(18)
            size_hint: 0.9,0.9
            text_size: (self.width,None)
            padding: dp(30)

        Button:
            text: 'Cerrar Mensaje'
            color: 1,1,1,1
            size_hint: 0.8,None
            pos_hint: {'center_x':0.5}
            padding: dp(30)
            height: dp(30)
            background_color: (0.3,0.3,0.7,1)
            on_release: root.dismiss()


<Etiquetas_Elementos@BoxLayout>:
    orientation: 'horizontal'
    size_hint_y: None
    text: 'Sin Texto'
    height: dp(30)
    padding: dp(5)
    canvas.before:
        Color: 
            rgba: 0.7,0.7,0.7,1
        Line: 
            points: self.x,self.y,self.x+self.width,self.y
    CheckBox:
        id: seleccion
        active: False
        size_hint: None, 0.9
        width: self.height
        pos_hint: {'center_y': 0.5}
        background_checkbox_down: ''
        background_checkbox_disabled_down: ''
        background_radio_down: ''
        
        canvas: 
            Color:
                rgba: (0, 0, 0, 1) if self.active else (0.7, 0.7, 0.7, 1)
            Ellipse:
                pos: self.pos
                size: self.size
    Label:
        text: root.text
        font_size: root.height*0.7
        color: 0,0,0,1

